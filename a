+-- own_window_argb_value 50
+
+minimum_width = 400,
+minimum_height = 240,
+maximum_width = 400,
+
+gap_x = 10,--## left | right
+gap_y = 40,--## up | down
+
+alignment = 'top_left',
+--######################  End Window Settings  ###
+--##  Font Settings  #############################
+-- Use Xft (anti-aliased font and stuff)
+use_xft = true,
+--# Requires mono font for spacing reasons
+--xftfont Liberation Mono:bold:size=9
+font = 'Roboto:size=10',
