+	},
+	{
+		name='cpu', -- dummy (used for arc)
+		arg='',
+		max=1,
+		bg_colour=0xffffff,
+		bg_alpha=0.7,
+		fg_colour=0xffffff,
+		fg_alpha=0,
+		x=266, y=155,
