+	color0 = '#B0E0E6',
+	color1 = '#778899',
+	color2 = '#D8BFD8',
+	color3 = '#cccccc',
+	color4 = '#BDBDBD',
+	color5 = '#CCCCCC',
+	color6 = '#aa0000',
